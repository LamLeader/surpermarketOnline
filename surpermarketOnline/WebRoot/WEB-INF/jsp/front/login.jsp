<%@ page language="java" pageEncoding="UTF-8"%><!DOCTYPE html><html>	<%@include file="inc/inc_css.jsp" %>	<script src="${ctx}/resource/front/js/login.js"></script>		<link rel="stylesheet" href="${ctx}/resource/front/css/login.css" />	<body onload="loadCookie()">		<!--header-->		<%@include file="inc/inc_head.jsp" %>		<div class="content jumbotron">			<div class="container">
				<div class="col-md-4 col-sm-6 col-md-offset-4 col-sm-offset-3 col-xs-12 imfor">					<h3>会员登录</h3>					<form name="form1" id="ff1"  class="form-horizontal" method="post">						<div class="input-group">							<span class="input-group-btn">								<img src="${ctx}/resource/front/images/login_img/person.png" />							</span>							<input type="text" class="form-control" id="login_username" name="username" placeholder="账号">						</div><br />						<div class="input-group">							<span class="input-group-btn">								<img src="${ctx}/resource/front/images/login_img/person.png"  />							</span>							<input type="password" class="form-control" id="login_password" name="password" placeholder="密码">						</div><br />						<button type="button"  onclick="loginCheck();" class="btn"/>登&nbsp &nbsp录</button>					   	<span class="pass">					   		<a href="register.html" class="new_user">注册新账户</a>/<a href="find.html">忘记密码？</a>					   	</span>					</form>				</div>
			</div>		</div>		<!--content end-->	<%@include file="inc/inc_foot.jsp" %>			</body>	<script type="text/javascript">	function loginCheck(){		var username = $("#login_username").val();		var password = $("#login_password").val();		var type = $("#type").val();		if(username==''||password==''){			alert('用户名和密码必须填写');			return false;		}		var aa="";		var bb="";		if(1==2){			aa="${ctx}/adminLogin/save.html";			bb="${ctx}/admin/index.html";		}else{						aa="${ctx}/front/save.html";			bb="${ctx}/front/index.html";		}		$.ajax({		      type: "POST",		      async:false,  // 设置同步方式		      cache:false,		      url: aa,				data:{username:username,password:password},				success:function(result){				result = eval("("+result+")");				if(result.status=='true'||result.status==true){					if(result.msg=='1'){						alert('登录成功');						window.location.href=bb;					}else if(result.msg=='0'){						alert('密码或用户名错误');					}				}		      }			});	}		</script></html>